{
  "name": "@ivex0002/stack-modal-presets",
  "version": "1.0.4",
  "description": "Preset layouts for @ivex0002/stack-modal library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "stack-modal",
    "stack-modal-presets",
    "react",
    "modal",
    "preset",
    "layout",
    "ui"
  ],
  "author": "ivex0002",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ivex0002/stack-modal.git",
    "directory": "packages/stack-modal-presets"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "pack:test": "npm run build && npm pack"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "@ivex0002/stack-modal": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "@ivex0002/stack-modal": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
